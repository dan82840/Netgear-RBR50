config config repacd
	option Enable                           '1'
	option ManagedNetwork                   '0'
	option DeviceType                       'RE'
	option Role                             'NonCAP'
	option GatewayConnectedMode             'AP'
	option ConfigREMode                     'wds'
	option DefaultREMode                    'son'
	option BlockDFSChannels                 '0'
