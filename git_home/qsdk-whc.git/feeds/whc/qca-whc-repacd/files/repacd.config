	option RateNumMeasurements              '5'
	option RateThresholdMin5G               '700'
	option RateThresholdMax5G               '1200'
	option RateThresholdPrefer2GBackhaul    '100'
	option 5GBackhaulBadlinkTimeout         '60'
	option BSSIDAssociationTimeout          '90'
	option RateScalingFactor                '70'

